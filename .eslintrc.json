{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "ignorePatterns": [
    "**/*.d.js",
    "node_modules/**/*"
  ],
  "rules": {
    "quote-props": "warn"
  },
  "globals": {
    "fetch": false,
    "Deno": false
  }
}